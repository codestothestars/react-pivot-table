        "lines": 100,
        "statements": 100
      }
    }
  },
  "keywords": [
    "pivot",
    "react",
    "table"
  ],
  "license": "MIT",
  "main": "lib/lib.js",
  "name": "@codestothestars/react-pivot-table",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codestothestars/react-pivot-table.git"
  },
  "scripts": {
    "build": "tsc --project src/tsconfig.dist.json",
    "eject": "react-scripts eject",
    "lint": "eslint \"src/**/*.ts*\"",
    "prepare": "npm run build",
    "start": "react-scripts start",
    "test": "react-scripts test --env=jsdom-fourteen"
  },
  "version": "0.0.1"
